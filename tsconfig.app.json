  "include": ["frontend/env.d.ts", "frontend/src/**/*", "frontend/src/**/*.vue"],
  "exclude": ["frontend/src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "paths": {
      "@/*": ["./frontend/src/*", "./backend/src/*"]
    },
    "target": "ES6"
  }
}
